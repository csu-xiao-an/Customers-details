@import "../../variables.css"
body
  font-family var(--font-family)
  font-size 14px
  line-height 1.42857143
  height 100%
  color var(--main-color)
  background-color #dedeff
  margin 0
.modal-header
  background-color var(--main-bgcolor)
  height 50px
  border-top-left-radius 10px
  border-top-right-radius 10px
  img
    display inline-block
    width 30px
    margin 10px
    transform rotate(45deg)
  h1
    display inline-block
    width 280px
    margin-top 15px
    text-align center
    color white
    font-weight 500
  .pd-l
    padding-left 54px
  .pd-r
    padding-right 54px
#home
  overflow hidden
  width 100%
  .test
    height 1000px
  .main-button
    margin 0 7px 1px
    button
      width 100%
      height 3.077em
      border none
      margin 0 auto
      font-weight 600
      font-size 1.3em
      color var(--dark-color)
      border-top-right-radius 8px
      border-top-left-radius 8px
      background-color white
